<%= visual_effect(:appear, "study_plan#{study_plan.id}") %>
<%= update_element_function("study_plan#{study_plan.id}", :position => :top,
:content => render(:partial => "shared/subjects", 
:locals => {:subjects => subjects, :title => _("subjects")})) %>
<%= update_element_function("study_plan#{study_plan.id}", :position => :before,
:content => approve_links(study_plan, 'study_plans')) %>
<%= update_element_function("subject_link#{study_plan.id}", :action => :remove) %>

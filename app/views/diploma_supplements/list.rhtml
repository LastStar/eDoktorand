<% unless @diploma_supplements.empty? %>
<h5><%= t(:message_0, :scope => [:view, :diploma_supplements, :list]) %></h5>

<table id='diploma_table'>
  <tr>
    <th><%= t(:message_1, :scope => [:view, :diploma_supplements, :list]) %></th>
    <th><%= t(:message_2, :scope => [:view, :diploma_supplements, :list]) %></th>
    <th><%= t(:message_3, :scope => [:view, :diploma_supplements, :list]) %></th>
    <th><%= t(:message_4, :scope => [:view, :diploma_supplements, :list]) %></th>
    <th class='action' colspan='3'><%= t(:message_5, :scope => [:view, :diploma_supplements, :list]) %></th>
  </tr>

<% for diploma_supplement in @diploma_supplements %>
  <tr>
  <% for column in ['diploma_no', 'family_name', 'given_name', 'study_specialization'] %>
    <td><%=h diploma_supplement.send(column) %></td>
  <% end %>
    <td><%= link_to t(:message_6, :scope => [:view, :diploma_supplements, :list]), :controller => :documents, :action => 'diploma_supplement', :id => diploma_supplement %></td>
    <td><%= link_to t(:message_7, :scope => [:view, :diploma_supplements, :list]), :action => 'edit', :id => diploma_supplement %></td>
    <td><%= link_to t(:message_8, :scope => [:view, :diploma_supplements, :list]), { :action => 'destroy', :id => diploma_supplement }, :confirm => t(:message_9, :scope => [:view, :diploma_supplements, :list]), :method => :post %></td>
  </tr>
<% end %>
</table>

<% else %>
<h4><%= t(:message_10, :scope => [:view, :diploma_supplements, :list]) %></h4>
<% end %>
<br />

<%= link_to t(:message_11, :scope => [:view, :diploma_supplements, :list]), :action => 'new' %>

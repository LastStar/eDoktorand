<ul class='peopleList'>
<% for index in @indexes  %>
  <li>
  <div class='right'>
  <%= render_partial 'student_links', :index => index %>
  </div>
  <div>
  <b><%= index.student.display_name %></b>
  </div>
  <div class='detail' id='info<%= index.id %>' style="display: none">
  </div >
  <% if index.study_plan %>
  <div id='study_plans<%= index.study_plan.id %>'>
  </div>
  <% end %>
  </li>
<% end %>
</ul>


<ul class='peopleList'>
<% for index in @indices  %>
  <li>
  <div class='right'>
    <%= student_action_link(index) %>
  </div>
  <div>
  <b><%= index.student.display_name %></b>
  </div>
  <div id='info<%= index.id %>'>
  </div >
  <% if index.study_plan %>
  <div id='study_plans<%= index.study_plan.id %>'>
  </div>
  <% end %>
  </li>
<% end %>
</ul>


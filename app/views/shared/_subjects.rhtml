<li><b><%= title %></b></li>
<% for subject in subjects %>
<li class='second'>
<div class='info'><%= subject.finishing_on %>.<%= _("semester") %></div>
<%= subject.subject.label %>
</li>
<% if subject.finished? %>
<li class='second'>
<%= "#{_("date finished")}: #{subject.finished_on}" %>
</li>
<% end %>
<% end %>


<h5><%= _("Editing probation term.") %></h5>
<div id="form">
<% form_tag :action => 'update' do -%>
  <%= hidden_field :probation_term, :id %>
  <%= render(:partial => 'subject_form', :locals => {:subjects => @subjects}) %>
  <%= render(:partial => 'probation_term_details_form') %>
  <%= render(:partial => 'examinators_form')%>
<% end -%>
</div>

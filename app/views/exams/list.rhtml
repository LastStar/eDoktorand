<%= javascript_include_tag :defaults %>
<% unless @exams.empty? %>
<h5 id="exam_title"><%= @title + " (%s)" % @exams.size %></h5>
<div id="exams_list">
  <%= list_links %>
  <%= render(:partial => 'list', :exams => @exams)%>
</div>
<div id='done'>
</div>
<div id='form'>
</div>
<% else %>
<h5><%= t(:message_0, :scope => [:txt, :view, :exams, :list, :rhtml]) %></h5>
<%= list_links %>
<% end %>

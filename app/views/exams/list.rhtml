<%= javascript_include_tag :defaults %>
<% unless @exams.empty? %>
<h5><%= @title + " (%s)" % @exams.size %></h5>
<div id="exams_list">
  <%= list_links %>
  <%= render(:partial => @partial)%>
</div>
<div id='done'>
</div>
<div id='form'>
</div>
<% else %>
<h5><%= _("There are no exams for you") %></h5>
<%= list_links %>
<% end %>
